{
  "dashboard": {
    "id": null,
    "title": "Болтай и Знакомься - Метрики приложения",
    "tags": ["boltaiznakomsya", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Запросы",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Время ответа HTTP",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95-й перцентиль"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50-й перцентиль"
          }
        ],
        "yAxes": [
          {
            "label": "Время (секунды)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Активные пользователи",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Активные пользователи"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Активные звонки",
        "type": "stat",
        "targets": [
          {
            "expr": "active_calls_total",
            "legendFormat": "Активные звонки"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "WebSocket соединения",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Активные соединения"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Попытки аутентификации",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(auth_attempts_total[5m])",
            "legendFormat": "{{type}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Попытки в секунду",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Регистрации пользователей",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_registrations_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Регистрации в секунду",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Жалобы на пользователей",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_reports_total[5m])",
            "legendFormat": "{{reason}}"
          }
        ],
        "yAxes": [
          {
            "label": "Жалобы в секунду",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Использование памяти",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_usage_bytes{type=\"heap\"}",
            "legendFormat": "Heap память"
          },
          {
            "expr": "memory_usage_bytes{type=\"process\"}",
            "legendFormat": "Процесс память"
          }
        ],
        "yAxes": [
          {
            "label": "Байты",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Использование CPU",
        "type": "graph",
        "targets": [
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "CPU %"
          }
        ],
        "yAxes": [
          {
            "label": "Проценты",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Ошибки",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{type}} - {{severity}}"
          }
        ],
        "yAxes": [
          {
            "label": "Ошибки в секунду",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 12,
        "title": "Длительность звонков",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(call_duration_seconds_bucket[5m]))",
            "legendFormat": "95-й перцентиль"
          },
          {
            "expr": "histogram_quantile(0.50, rate(call_duration_seconds_bucket[5m]))",
            "legendFormat": "50-й перцентиль"
          }
        ],
        "yAxes": [
          {
            "label": "Время (секунды)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
} 